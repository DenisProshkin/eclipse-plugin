<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.zephyrproject.ide.eclipse.ui.preferences.ZephyrPreferenceTopPage"
            class="org.zephyrproject.ide.eclipse.ui.preferences.ZephyrPreferenceToolchainPage"
            id="org.zephyrproject.ide.eclipse.ui.preferences.ZephyrPreferenceToolchainPage"
            name="Zephyr Toolchain">
      </page>
      <page
            class="org.zephyrproject.ide.eclipse.ui.preferences.ZephyrPreferenceTopPage"
            id="org.zephyrproject.ide.eclipse.ui.preferences.ZephyrPreferenceTopPage"
            name="Zephyr">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.zephyrproject.ide.eclipse.ui.preferences.ZephyrPreferenceInitializer">
      </initializer>
   </extension>
   <extension
         id="zephyrApplicationBuilder"
         name="Zephyr Application Builder"
         point="org.eclipse.core.resources.builders">
      <builder
            callOnEmptyDelta="true"
            hasNature="true"
            isConfigurable="true">
         <run
               class="org.zephyrproject.ide.eclipse.core.ZephyrApplicationBuilder">
         </run>
      </builder>
   </extension>
   <extension
         id="zephyrApplicationNature"
         name="Zephyr Application Nature"
         point="org.eclipse.core.resources.natures">
      <requires-nature
            id="org.eclipse.cdt.core.cnature">
      </requires-nature>
      <runtime>
         <run
               class="org.zephyrproject.ide.eclipse.core.ZephyrApplicationNature">
         </run>
      </runtime>
      <builder
            id="org.zephyrproject.ide.eclipse.zephyrApplicationBuilder">
      </builder>
   </extension>
   <extension
         id="xmlProblem"
         name="XML Problem"
         point="org.eclipse.core.resources.markers">
      <super
            type="org.eclipse.core.resources.problemmarker">
      </super>
      <persistent
            value="true">
      </persistent>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="org.zephyrproject.ide.eclipse.category.ui.wizards"
            name="Zephyr Project">
      </category>
      <wizard
            category="org.zephyrproject.ide.eclipse.category.ui.wizards"
            class="org.zephyrproject.ide.eclipse.ui.wizards.ZephyrApplicationNewProjectWizard"
            finalPerspective="org.eclipse.cdt.ui.CPerspective"
            icon="icons/zephyr-kite-logo_32x32.png"
            id="org.zephyrproject.ide.eclipse.ui.wizards.ZephyrApplicationNewProjectWizard"
            name="Zephyr Application"
            project="true">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.ide.projectNatureImages">
      <image
            icon="icons/zephyr-project-icon.png"
            id="org.zephyrproject.ide.eclipse.core.natureImage"
            natureId="org.zephyrproject.ide.eclipse.zephyrApplicationNature">
      </image>
   </extension>
   <extension
         point="org.eclipse.tools.templates.ui.templates">
      <template
            icon="icons/zephyr-kite-logo_48x48.png"
            id="org.zephyrproject.ide.eclipse.ui.ZephyrApplicationNewProjectTemplate"
            label="Zephyr Application"
            wizard="org.zephyrproject.ide.eclipse.ui.wizards.ZephyrApplicationNewProjectWizard">
         <description>
            A new Zephyr Application
         </description>
         <tagReference
               id="org.zephyrproject.ide.eclipse.ui.tag">
         </tagReference>
         <tagReference
               id="org.eclipse.cdt.ui.cdtTag">
         </tagReference>
      </template>
      <tag
            id="org.zephyrproject.ide.eclipse.ui.tag"
            label="Zephyr">
      </tag>
   </extension>
   <extension
         point="org.eclipse.cdt.core.buildConfigProvider">
      <provider
            class="org.zephyrproject.ide.eclipse.core.build.ZephyrApplicationBuildConfigurationProvider"
            id="org.zephyrproject.ide.eclipse.core.build.zephyrApplicationBuildConfigProvider"
            natureId="org.zephyrproject.ide.eclipse.zephyrApplicationNature">
      </provider>
   </extension>
   <extension
         point="org.eclipse.cdt.core.toolChainProvider">
      <provider
            class="org.zephyrproject.ide.eclipse.core.build.ZephyrApplicationToolChainProvider"
            id="org.zephyrproject.ide.eclipse.core.build.zephyrApplicationToolChainProvider">
      </provider>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTypeImages">
      <launchConfigurationTypeImage
            configTypeID="org.zephyrproject.ide.eclipse.launch.applicationRunLaunchConfigurationType"
            icon="icons/zephyr-kite-logo_16x16.png"
            id="org.zephyrproject.ide.eclipse.launch.applicationRunLaunchConfigurationTypeImage">
      </launchConfigurationTypeImage>
   </extension>
   <extension
         point="org.eclipse.debug.core.launchConfigurationTypes">
      <launchConfigurationType
            delegate="org.zephyrproject.ide.eclipse.core.launch.ZephyrApplicationRunLaunchConfigDelegate"
            id="org.zephyrproject.ide.eclipse.launch.applicationRunLaunchConfigurationType"
            modes="run"
            name="Zephyr Application"
            public="true">
      </launchConfigurationType>
      <launchConfigurationType
            delegate="org.zephyrproject.ide.eclipse.core.launch.ZephyrApplicationRunLaunchConfigDelegate"
            id="org.zephyrproject.ide.eclipse.core.launchTargetType.emulator"
            modes="run"
            name="Emulator"
            public="false">
      </launchConfigurationType>
      <launchConfigurationType
            delegate="org.zephyrproject.ide.eclipse.core.launch.ZephyrApplicationRunLaunchConfigDelegate"
            id="org.zephyrproject.ide.eclipse.core.launchTargetType.hardware"
            modes="run"
            name="Hardware"
            public="false">
      </launchConfigurationType>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTabGroups">
      <launchConfigurationTabGroup
            class="org.zephyrproject.ide.eclipse.ui.launch.ZephyrApplicationRunLaunchConfigurationTabGroup"
            id="org.zephyrproject.ide.eclipse.launch.applicationRunLaunchConfigurationTabGroup"
            type="org.zephyrproject.ide.eclipse.launch.applicationRunLaunchConfigurationType">
         <launchMode
               mode="run">
         </launchMode>
      </launchConfigurationTabGroup>
   </extension>
   <extension
         point="org.eclipse.launchbar.core.launchBarContributions">
      <descriptorType
            class="org.zephyrproject.ide.eclipse.core.launchbar.ZephyrApplicationLaunchDescriptorType"
            id="org.zephyrproject.ide.eclipse.launch.descriptorType.default"
            priority="50">
         <enablement>
            <instanceof
                  value="org.eclipse.core.resources.IProject">
            </instanceof>
            <test
                  forcePluginActivation="true"
                  property="org.eclipse.core.resources.projectNature"
                  value="org.zephyrproject.ide.eclipse.zephyrApplicationNature">
            </test>
         </enablement>
      </descriptorType>
      <configProvider
            class="org.zephyrproject.ide.eclipse.core.launchbar.ZephyrApplicationLaunchConfigurationProvider"
            descriptorType="org.zephyrproject.ide.eclipse.launch.descriptorType.default"
            priority="50">
      </configProvider>
   </extension>
   <extension
         point="org.eclipse.launchbar.ui.launchBarUIContributions">
      <descriptorUI
            descriptorTypeId="org.zephyrproject.ide.eclipse.launch.descriptorType.default"
            labelProvider="org.zephyrproject.ide.eclipse.core.launchbar.ZephyrApplicationLaunchBarConfigurationLabelProvider">
      </descriptorUI>
   </extension>
   <extension
         point="org.eclipse.launchbar.core.launchTargetTypes">
      <launchTargetType
            id="org.zephyrproject.ide.eclipse.core.launchTargetType.emulator"
            provider="org.zephyrproject.ide.eclipse.core.launchbar.ZephyrApplicationLaunchTargetProvider">
      </launchTargetType>
      <launchTargetType
            id="org.zephyrproject.ide.eclipse.core.launchTargetType.hardware"
            provider="org.zephyrproject.ide.eclipse.core.launchbar.ZephyrApplicationLaunchTargetProvider">
      </launchTargetType>
   </extension>
   <extension
         point="org.eclipse.launchbar.ui.launchTargetTypeUI">
      <launchTargetTypeUI
            id="org.zephyrproject.ide.eclipse.core.launchTargetType.emulator"
            labelProvider="org.zephyrproject.ide.eclipse.core.launchbar.ZephyrApplicationLaunchBarTargetLabelProvider">
      </launchTargetTypeUI>
      <launchTargetTypeUI
            id="org.zephyrproject.ide.eclipse.core.launchTargetType.hardware"
            labelProvider="org.zephyrproject.ide.eclipse.core.launchbar.ZephyrApplicationLaunchBarTargetLabelProvider">
      </launchTargetTypeUI>
   </extension>
</plugin>
